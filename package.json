    "npm-check-updates": "^12.5.2",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "react-docgen-typescript": "^2.2.2",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  },
  "jest": {
    "roots": [
      "src",
      "libs"
    ],
    "projects": [
      "src/tests/jest.config.js",
      "libs/jest.config.js"
    ]
  }
