    "apollo-server-micro": "^3.6.3",
    "chart.js": "^3.7.1",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.8",
    "deep-equal": "^2.0.5",
    "deepmerge": "^4.2.2",
    "graphql": "^16.3.0",
    "graphql-relay": "^0.10.0",
    "immer": "^9.0.12",
    "joi": "^17.6.0",
    "micro": "^9.3.4",
    "nanoid": "^3.3.1",
    "next": "^12.1.0",
    "next-auth": "^4.2.1",
    "next-themes": "^0.1.1",
    "nexus": "^1.3.0",
    "phin": "^3.6.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.1",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.27.1",
    "react-perfect-scrollbar": "^1.5.8",
    "recoil": "^0.6.1",
