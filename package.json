{
  "name": "nestjs-canary-boilerplate",
  "author": "snilli",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev -p 3333",
    "build": "next build",
    "start": "next start",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf .next/",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "next lint",
    "test": "jest -i",
    "test:clear": "jest --clearCache",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "storybook": "start-storybook -p 6666",
    "build-storybook": "build-storybook",
    "analyze": "cross-env ANALYZE=true yarn build",
    "update-package": "ncu -u --interactive --format group"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.9.3",
    "@hookform/resolvers": "^2.9.3",
    "react-icons":"^4.4.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.6",
    "@mui/x-data-grid": "^5.12.3",
    "@react-hook/window-scroll": "^1.3.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.11.3",
    "deep-equal": "^2.0.5",
    "deepmerge": "^4.2.2",
    "got": "^12.1.0",
    "immer": "^9.0.15",
    "joi": "^17.6.0",
    "nanoid": "^4.0.0",
    "next": "^12.2.0",
    "next-auth": "^4.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "react-perfect-scrollbar": "^1.5.8",
    "swiper": "^8.3.0",
    "use-immer": "^0.7.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.2.0",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/react": "6.5.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "eslint-config-next": "^12.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "8.0.1",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "npm-check-updates": "^15.0.1",
    "prettier": "^2.7.1",
    "react-docgen-typescript": "^2.2.2",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  },
  "jest": {
    "roots": [
      "src",
      "libs"
    ],
    "projects": [
      "src/tests/jest.config.js",
      "libs/jest.config.js"
    ]
  }
}
